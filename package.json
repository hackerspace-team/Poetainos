{
  "name": "poetainos",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write resources/js/",
    "up": "cd laradock && docker-compose up -d nginx mariadb workspace && docker-compose exec --user=laradock workspace bash",
    "down": "cd laradock && docker-compose stop",
    "restart": "npm run workspace-stop && npm run workspace-start",
    "translate": "php artisan translatable:export es",
    "translate-check": "php artisan translatable:inspect-translations es"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "@inertiajs/vue3": "^1.0.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "autoprefixer": "^10.4.12",
    "axios": "^1.6.2",
    "crop-url": "^4.0.1",
    "date-fns": "^3.1.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "laravel-echo": "^1.15.3",
    "laravel-vite-plugin": "^1.0.0",
    "linkify-html": "^4.1.3",
    "linkify-plugin-mention": "^4.1.3",
    "linkifyjs": "^4.1.3",
    "lodash": "^4.17.21",
    "markdown-it": "^14.0.0",
    "millify": "^6.1.0",
    "prettier": "^3.0.3",
    "pusher-js": "^8.4.0-rc2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.0",
    "vite-plugin-vuetify": "^2.0.1",
    "vue": "^3.2.41",
    "vue-i18n": "^9.8.0",
    "vuetify": "^3.4.8"
  }
}
